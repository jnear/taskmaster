    <div class="container">
      <div class="page-header">
	<h1>Taskmaster</h1>
      </div>
      
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">New Task</h3>
  </div>
  <div class="panel-body">
    
<form class="form-horizontal" role="form" id="newTaskForm">
  
  <div class="form-group">
    <label for="inputNewTask1" class="col-sm-2 control-label">Description</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputNewTaskDescription" placeholder="Task Description" name="desc">
    </div>
  </div>

  <div class="form-group">
    <label for="inputNewTask2" class="col-sm-2 control-label">Domain</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputNewTaskDomain" placeholder="Allowed Domains" name="domain">
    </div>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default" id="newTaskButton">Create Task</button>
    </div>
  </div>
</form>
  </div>
</div>

